{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs Debug",
            "probe": "303a:1001", //!MODIFY (or remove)
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "cargo build --example",
            "connectUnderReset": false,
            "chip": "esp32c3",
            "flashingConfig": {
                "flashingEnabled": true,
                "resetAfterFlashing": true,
                "haltAfterReset": false,
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "./target/riscv32imac-unknown-none-elf/debug/examples/${fileBasenameNoExtension}",
                    "rttEnabled": true,
                }
            ]
        },
        {
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs Release",
            "probe": "303a:1001", //!MODIFY (or remove)
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "cargo build --example --release",
            "connectUnderReset": false,
            "chip": "esp32c3",
            "flashingConfig": {
                "flashingEnabled": true,
                "resetAfterFlashing": true,
                "haltAfterReset": false
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "./target/riscv32imac-unknown-none-elf/release/examples/${fileBasenameNoExtension}",
                    "rttEnabled": true,
            }
        ]
    }
]
}